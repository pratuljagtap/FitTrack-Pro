{
  "name": "WorkoutSet",
  "type": "object",
  "properties": {
    "workout_id": {
      "type": "string",
      "description": "ID of the workout this set belongs to"
    },
    "exercise_id": {
      "type": "string",
      "description": "ID of the exercise performed"
    },
    "exercise_name": {
      "type": "string",
      "description": "Name of exercise for easy display"
    },
    "set_number": {
      "type": "number",
      "description": "Set number in the workout"
    },
    "reps": {
      "type": "number",
      "description": "Number of repetitions"
    },
    "weight": {
      "type": "number",
      "description": "Weight used in pounds or kg"
    },
    "rest_seconds": {
      "type": "number",
      "description": "Rest time after the set in seconds"
    },
    "rpe": {
      "type": "number",
      "description": "Rate of Perceived Exertion (1-10)"
    }
  },
  "required": [
    "workout_id",
    "exercise_id",
    "exercise_name",
    "set_number"
  ]
}
